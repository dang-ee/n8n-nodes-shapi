{
  "name": "n8n-nodes-shapi",
  "version": "0.1.0",
  "description": "n8n custom node for SHAPI integration",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "dang-ee",
    "email": "kroivanian@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dang-ee/n8n-nodes-shapi.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ShapiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Shapi/Shapi.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.45.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "n8n-core": "*"
  }
}
